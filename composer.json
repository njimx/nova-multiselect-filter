{
  "name": "njimx/nova-multiselect-filter",
  "description": "A Laravel Nova filter that allows multiple selection.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergii Kauk",
      "email": "sergii@kauk.at",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "laravel/nova": "^4.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "rcknr\\Nova\\Filters\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "rcknr\\Nova\\Filters\\FilterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
